{
  "name": "@pinata/sdk",
  "version": "1.1.12",
  "description": "Official SDK for the Pinata IPFS platform",
  "main": "lib/pinata-sdk.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "coverage": "jest --coverage",
    "dev": "webpack --progress --colors --watch --env dev",
    "html": "open coverage/index.html",
    "lint": "eslint src --quiet ",
    "lint:fix": "eslint src --fix",
    "test": "jest",
    "repl": "node -i -e \"$(< ./lib/pinata-sdk.js)\"",
    "preinstall": "npx npm-force-resolutions"
  },
  "resolutions": {
    "serialize-javascript": "^5.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PinataCloud/Pinata-SDK.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs"
  ],
  "author": "Matt Ober",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PinataCloud/Pinata-SDK/issues"
  },
  "homepage": "https://github.com/PinataCloud/Pinata-SDK",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.0",
    "@babel/register": "^7.13.16",
    "acorn": "^8.2.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-polyfill": "^6.26.0",
    "eslint": "^7.25.0",
    "eslint-loader": "^3.0.3",
    "jest": "^26.6.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0",
    "yargs": "^16.2.0"
  },
  "dependencies": {
    "base-path-converter": "^1.0.2",
    "form-data": "^4.0.0",
    "is-ipfs": "1.0.3",
    "recursive-fs": "^2.1.0",
    "redaxios": "^0.4.1"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "text",
      "html"
    ],
    "moduleFileExtensions": [
      "js"
    ]
  }
}
